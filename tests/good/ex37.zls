let g(x) =
  let m = (let z = 1 in z + 1)
  in let h = m + 1 in
     h + m

let h(x) returns (o)
   local y
   do
    local m do y = 2 and m = y done
   and
    o = let m = (let z = 1 in
                 z + 1)
        in let h = m + 1 in
           h + m
   done


