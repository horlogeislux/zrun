let f x =
  let y = x + 1 in y + 2

let g x = inline f (x + 1)

let h x =
  let z = x + inline g (x + 2) in
  if x = 0 then inline f (z + 3) else inline g (x - 1)
    
let l x returns o
  automaton
  | S1 -> do o = inline f (x + 1) until (o = 2) then S2
  | S2 -> do reset o = inline f (x + 2) every inline f (x + 3) done
  end

